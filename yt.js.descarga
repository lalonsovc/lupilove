function toggleAudio(){if(player.getPlayerState()==1||player.getPlayerState()==3){player.pauseVideo();togglePlayButton(false);apagarMarque();}else{player.playVideo();togglePlayButton(true);prenderMarque();}}
function onPlayerReady(event){player.setPlaybackQuality("small");document.getElementById("youtube-audio").style.display="inline-block";togglePlayButton(player.getPlayerState()!==5);if(player.getDuration()===0){PlayButtonAdvertencia();marqueMusic.innerHTML="\u{1F507} Audio No disponible \u{1F507}";}}
function onPlayerStateChange(event){if(event.data===1){prenderMarque();}
if(event.data===0){togglePlayButton(false);apagarMarque();}}
function onPlayerError(event){if(event.data===5){PlayButtonAdvertencia();marqueMusic.innerHTML="\u{1F507} Audio No disponible \u{1F507}";}
if(event.data===100){PlayButtonAdvertencia();marqueMusic.innerHTML="\u{1F507} Audio No disponible \u{1F507}";}
if(event.data===101){PlayButtonAdvertencia();marqueMusic.innerHTML="\u{1F507} Audio No disponible \u{1F507}";}
if(event.data===150){PlayButtonAdvertencia();marqueMusic.innerHTML="\u{1F507} Audio No disponible \u{1F507}";}
console.log("error: "+event.data);}
function apagarMarque(){marque.classList.remove("marquee-mus-title");marque.classList.add("marquee-apagado");}
function prenderMarque(){marque.classList.add("marquee-mus-title");marque.classList.remove("marquee-apagado");}